<h1>Bird Index</h1>
<a href="/flocks">Return to all Flocks</a><br>
<a href="/birds">Return to all Birds</a><br><br>

<% @birds.each do | bird | %>
  <h3><%= bird.name %></h3>
  <table>
    <% bird.attributes.each_pair do | key, value | %>
      <tr>
        <td><%= key %>: </td>
        <td><%= value %></td>
      </tr>
    <% end %>
  </table>
  <br><br>
<% end %>